{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
        "$app": false,
        "$config": false,
        "$logger":false
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "args": "none",
                "ignoreRestSiblings": false
            }
        ]
    }
}